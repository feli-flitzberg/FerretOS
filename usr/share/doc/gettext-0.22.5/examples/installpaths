prefix='/usr'
exec_prefix="${prefix}"
datarootdir="${prefix}/share"
datadir="${datarootdir}"
docdir="/usr/share/doc/gettext-0.22.5"
ACLOCALDIR="${datadir}/aclocal"
GETTEXTSRCDIR="${datarootdir}/gettext"
GETTEXTSRCPODIR="${datarootdir}/gettext/po"
EXAMPLESCONFIGDIR="/usr/share/doc/gettext-0.22.5/examples/config"
